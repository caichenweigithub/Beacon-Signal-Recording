<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.test.MainActivity" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/it3_text1"
        android:gravity="left|center_vertical"
        android:orientation="horizontal"
        android:weightSum="720" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical"
            android:paddingLeft="2dp"
            android:singleLine="true"
            android:text="Major : "
            android:textColor="#FFFFFF" />

        <TextView
            android:id="@+id/it3_text2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="220"
            android:gravity="center"
            android:singleLine="true"
            android:textColor="#FFFFFF" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical"
            android:paddingLeft="2dp"
            android:singleLine="true"
            android:text="Minor : "
            android:textColor="#FFFFFF" />

        <TextView
            android:id="@+id/it3_text3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="220"
            android:gravity="center"
            android:singleLine="true"
            android:textColor="#FFFFFF" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left|center_vertical"
        android:orientation="horizontal"
        android:weightSum="720" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical"
            android:paddingLeft="2dp"
            android:singleLine="true"
            android:text="rssi : "
            android:textColor="#FFFFFF" />

        <TextView
            android:id="@+id/it3_text4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="561.26"
            android:gravity="center"
            android:singleLine="true"
            android:textColor="#FFFFFF" />
    
    </LinearLayout>

    <TextView
        android:id="@+id/it3_text1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/linearLayout2"
        android:layout_below="@+id/linearLayout2"
        android:gravity="left|center_vertical"
        android:singleLine="true"
        android:textColor="#FFFFFF"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/linearLayout1"
        android:layout_centerVertical="true"
        android:src="@drawable/abc_ab_share_pack_holo_dark" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView1"
        android:layout_alignLeft="@+id/imageView1"
        android:text="TextView"
        android:textColor="#FFFFFF" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView1"
        android:layout_marginLeft="80dp"
        android:layout_toRightOf="@+id/textView1"
        android:background="@drawable/ic_launcher"
        android:src="@drawable/abc_ab_share_pack_holo_dark"
        android:visibility="visible" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_below="@+id/imageView6"
        android:layout_marginTop="34dp"
        android:background="@drawable/ic_launcher"
        android:src="@drawable/abc_ab_share_pack_holo_dark" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView3"
        android:layout_centerVertical="true"
        android:background="@drawable/ic_launcher"
        android:src="@drawable/abc_ab_share_pack_holo_dark" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView2"
        android:layout_below="@+id/imageView4"
        android:layout_marginTop="47dp"
        android:background="@drawable/ic_launcher"
        android:src="@drawable/abc_ab_share_pack_holo_dark"
        android:visibility="visible" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_launcher"
        android:src="@drawable/abc_ab_share_pack_holo_dark"
        android:layout_below="@+id/linearLayout1"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5" />

    <Button
        android:text="store"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/store" />

    <TextView
        android:id="@+id/minor1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor1"
        android:layout_toEndOf="@+id/minor1"
        android:layout_toRightOf="@+id/minor1"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor2"
        android:layout_toEndOf="@+id/minor2"
        android:layout_toRightOf="@+id/minor2"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor3"
        android:layout_toEndOf="@+id/minor3"
        android:layout_toRightOf="@+id/minor3"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@android:color/white"
        android:layout_below="@+id/minor1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/minor6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor5"
        android:layout_toEndOf="@+id/minor5"
        android:layout_toRightOf="@+id/minor5"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor6"
        android:layout_toEndOf="@+id/minor6"
        android:layout_toRightOf="@+id/minor6"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor7"
        android:layout_toEndOf="@+id/minor7"
        android:layout_toRightOf="@+id/minor7"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@android:color/white"
        android:layout_below="@+id/minor5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/minor10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor9"
        android:layout_toEndOf="@+id/minor9"
        android:layout_toRightOf="@+id/minor9"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor10"
        android:layout_toEndOf="@+id/minor10"
        android:layout_toRightOf="@+id/minor10"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor11"
        android:layout_toEndOf="@+id/minor11"
        android:layout_toRightOf="@+id/minor11"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="@android:color/white"
        android:layout_alignTop="@+id/imageView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/minor14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor13"
        android:layout_toEndOf="@+id/minor13"
        android:layout_toRightOf="@+id/minor13"
        android:text="TextView"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/minor15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor14"
        android:layout_toEndOf="@+id/minor14"
        android:layout_toRightOf="@+id/minor14"
