    <TextView
        android:id="@+id/minor16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minor15"
        android:layout_toEndOf="@+id/minor15"
        android:layout_toRightOf="@+id/minor15"
